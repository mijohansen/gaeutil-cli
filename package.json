  "bugs": {
    "url": "https://github.com/mijohansen/node-gae-util/issues"
  },
  "homepage": "https://github.com/mijohansen/node-gae-util#readme",
  "dependencies": {
    "@google-cloud/storage": "^1.6.0",
    "commander": "^2.15.1",
    "googleapis": "^27.0.0",
    "js-yaml": "^3.11.0",
    "jsonfile": "^4.0.0",
    "xmldom": "^0.1.27"
  },
  "bin": {
    "gaeutil": "./index.js"
  }
